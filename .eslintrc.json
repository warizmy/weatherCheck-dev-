{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-restricted-globals": 0,
    "linebreak-style": 0,
    "no-param-reassign": 0,
    "no-undef": 0,
    "no-sequences": 0,
    "no-shadow": 0,
    "no-return-assign": 0,
    "no-unused-expressions": 0,
    "import/no-amd": 0,
    "no-plusplus": 0,
    "class-methods-use-this": 0
  }
}
